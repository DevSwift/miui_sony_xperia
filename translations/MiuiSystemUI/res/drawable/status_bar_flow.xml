<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <bitmap android:gravity="center" android:src="@drawable/status_bar_flow_bg" />
    </item>
    <item android:id="@id/flow_rotate">
        <rotate android:useLevel="false" android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <shape android:shape="ring" android:thicknessRatio="2.0" android:useLevel="true" android:innerRadius="0.0dip"
              xmlns:android="http://schemas.android.com/apk/res/android" />
        </rotate>
    </item>
</layer-list>
