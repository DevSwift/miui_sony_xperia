    <string name="pa_done_txt">完成</string>
    <string name="pa_facebook_xperia_title">"Xperia™內的Facebook "</string>
    <string name="pa_facebook_xperia_desc">將Facebook與行事曆、音樂播放器、相簿等功能整合。</string>
    <string name="pa_google_talk_title">Google Talk</string>
    <string name="pa_timescape_title">Timescape™</string>
