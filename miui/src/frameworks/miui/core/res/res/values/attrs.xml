<?xml version="1.0" encoding="utf-8"?>
<!-- Formatting note: terminate all comments with a period, to avoid breaking
     the documentation output. To suppress comment lines from the documentation
     output, insert an eat-comment element after the comment lines.
-->

<resources>
      <!-- @hide -->
    <declare-styleable name="SpectrumVisualizer">
      <!-- @hide -->
      <attr name="symmetry" format="boolean"/>
      <!-- @hide -->
      <attr name="sliding_dot_bar" format="reference"/>
      <!-- @hide -->
      <attr name="sliding_shadow_dot_bar" format="reference"/>
      <!-- @hide -->
      <attr name="sliding_panel" format="reference"/>
      <!-- @hide -->
      <attr name="alpha_width" format="integer"/>
      <!-- @hide -->
      <attr name="update_enable" format="boolean"/>
      <!-- @hide -->
      <attr name="care_streamactive" format="boolean"/>
   </declare-styleable>
    <!-- =============================== -->
    <!-- MiuiInCallAnswerWidgetHorizontal class attributes -->
    <!-- =============================== -->
    <declare-styleable name="MiuiInCallAnswerWidgetHorizontal">
        <!-- Reference to an array resource with light that be shown as targets around a circle.-->
        <attr name="targetDrawablesLight" format="reference"/>
        <attr name="rightHintAnimationDrawable" format="reference"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- SlidingButton attributes        -->
    <!-- =============================== -->
    <declare-styleable name="SlidingButton">
        <attr name="buttonFrame" format="reference"/>
        <attr name="buttonSlider" format="reference"/>
        <attr name="buttonSliderPressed" format="reference"/>
        <attr name="buttonOnDisable" format="reference"/>
        <attr name="buttonOffDisable" format="reference"/>
        <attr name="buttonMask" format="reference"/>
        <attr name="buttonBar" format="reference"/>
        <attr name="buttonBarOff" format="reference"/>
        <attr name="buttonBarOn" format="reference"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- GradientTextView attributes -->
    <!-- =============================== -->
    <declare-styleable name="GradientTextView">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- AlphabetFastIndexer attributes -->
    <!-- =============================== -->
    <declare-styleable name="AlphabetFastIndexer">
        <attr name="indexerTable" format="reference"/>
        <attr name="indexerTextSize" format="dimension"/>
        <attr name="indexerTextColor" format="color"/>
        <attr name="indexerTextHighlightColor" format="color"/>
        <attr name="indexerBackground" format="reference"/>
        <attr name="overlayBackground" format="reference"/>
        <attr name="overlayMarginLeft" format="dimension"/>
        <attr name="overlayMarginTop" format="dimension"/>
        <attr name="overlayTextSize" format="dimension"/>
        <attr name="overlayTextColor" format="color"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- PhotoFrameView attributes -->
    <!-- =============================== -->
    <declare-styleable name="PhotoFrameView">
        <attr name="boundType" >
            <enum name="outside" value="0" />
            <enum name="inside" value="1" />
        </attr>
        <attr name="insideLeft" format="dimension"/>
        <attr name="insideTop" format="dimension"/>
        <attr name="insideWidth" format="dimension"/>
        <attr name="insideHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="LockPatternView">
        <!-- Aspect to use when drawing LockPatternView. Choices are "square"(default), "lock_width"
             or "lock_height" -->
        <attr name="aspect" format="string" />

        <attr name="paintColor" format="color" />
        <attr name="diameterFactor" format="float" />
        <attr name="pathStrokeAlpha" format="integer" />

        <attr name="btnDefaultBmp" format="reference" />
        <attr name="btnDefaultBackgroundBmp" format="reference" />
        <attr name="btnTouchedBmp" format="reference" />
        <attr name="circleDefaultBmp" format="reference" />
        <attr name="circleGreenBmp" format="reference" />
        <attr name="circleRedBmp" format="reference" />
        <attr name="arrowGreenUpBmp" format="reference" />
        <attr name="arrowRedUpBmp" format="reference" />
    </declare-styleable>

    <!-- =============================== -->
    <!-- MIUI ROM APP micloud-push属性名， 需要和 @see MicloudPushConstants.MICLOUD_PUSH_ATTRI_NAME 同步修改 -->
    <!-- =============================== -->
    <declare-styleable name="MicloudPush">
        <attr name="contentAuthority" format="string"/>
        <attr name="pushType" format="string" />
        <attr name="pushName" format="string" />
        <attr name="capability" format="string" />
    </declare-styleable>

</resources>
